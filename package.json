{
  "name": "easy-validator-js",
  "version": "1.0.16",
  "description": "Easy validation of forms in using javascript",
  "main": "dist/validator-js.js",
  "keywords": [
    "validator",
    "js validator",
    "vuex validator",
    "redux validator"
  ],
  "scripts": {
    "test": "jest",
    "watch-test": "npm run test -- --watch --notify",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avinash403/validator-js.git"
  },
  "author": "avinash kumar <ak403602@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avinash403/easy-validator-js/issues"
  },
  "homepage": "https://github.com/avinash403/easy-validator-js#readme",
  "dependencies": {
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "roots": [
      "<rootDir>/tests/"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  }
